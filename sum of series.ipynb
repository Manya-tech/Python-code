    "def sum_series(n):\n",
    "    s=0\n",
    "    for i in range (1,n+1):\n",
    "        s=s+i\n",
    "    return s\n",
    "\n",
    "m=int(input('enter a number:'))\n",
    "su=sum_series(m)\n",
    "\n",
    "print('sum:',su)\n"
